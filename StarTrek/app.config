          <FactionThreat severity="normal" value="vubpu' jon nuchpu'. jonbe' tlhInganpu'." translation = "Cowards take hostages. Klingons do not." />
          <FactionThreat severity="normal" value="vaj toDuj Daj ngeHbej DI vI'." translation = "Shooting space garbage is no test of a warrior's mettle." />
          <FactionThreat severity="normal" value="qoH vuvbe' SuS." translation = "The wind does not respect a fool." />
          <FactionThreat severity="normal" value="vay' DaghIjlaHchugh bIHoSghaj." translation = "Fear is power." />
          <FactionThreat severity="normal" value="HIvbe' qoHpu' neH." translation = "Only fools don't attack." />
          <FactionThreat severity="normal" value="Hegh neH chav qoH." translation = "A fool's only achievement is death." />
          <FactionThreat severity="normal" value="bortaS nIvqu' 'oH bortaS'e'." translation = "Revenge is the best revenge." />
          <FactionThreat severity="normal" value="yInlu'taH 'e' bajnISlu'." translation = "Survival must be earned." />
          <FactionThreat severity="normal" value="potlhbe'chugh yay qatlh pe''eghlu'?" translation = "If winning is not important, then why keep score?" />
          <FactionThreat severity="normal" value="butlh DaHutlh." translation = "You lack dirt under your fingernails." />
          <FactionThreat severity="normal" value="HuH DaHutlh." translation = "you lack gall. (You lack bile.)" />
          <FactionThreat severity="normal" value="don't be a bitch" translation = "Qo', qatlh bitch" />
          <FactionThreat severity="normal" value="nuHlIj DawIvpu', vaj yISuv!" translation = "You have choosen your weapon, so fight!" />
          <FactionThreat severity="normal" value="QotDI' gheD tlhejbe' wamwI'." translation = "he hunter does not lie down with the prey." />
          <FactionThreat severity="normal" value="tlhIngan ngoQmey chavmeH HoH tlhInganpu'." translation = "Klingons kill for their own purposes." />
          <FactionThreat severity="normal" value="reH lugh charghwI'." translation = "The victor is always right." />
          <FactionThreat severity="normal" value="qun qon charghwI'pu''e'." translation = "History is written by the victors." />
          <FactionThreat severity="normal" value="noH ghoblu'DI' yay quv law' Hoch quv puS." translation = "In war, there is nothing more honorable than victory." />
          <FactionThreat severity="normal" value="maQapmeH maHIv." translation = "In order to succeed, we attack." />
          <FactionThreat severity="normal" value="rut yIHmey ghom Hoch." translation = "Everyone encounters tribbles occasionally." />
          <FactionThreat severity="normal" value="pIpyuS pach DaSop DaneHchugh; pIpyuS puS DaghornIS." translation = "If you want to eat pipius claw, you'll have to break a few pipiuses." />
          <FactionThreat severity="normal" value="yIQeqQo' neH. DoS yIqIp!" translation = "Don't just aim; hit the target!" />
          <FactionThreat severity="normal" value="'Iw bIQtIqDaq jIjaH." translation = "I travel the river of blood." />
          <FactionThreat severity="normal" value="loD Quch jIHbe'." translation = "I am not a merry man" />
          <FactionThreat severity="normal" value="QaghmeylIj tIchID, yIyoH." translation = "Have the courage to admit your mistakes." />
          <FactionThreat severity="normal" value="Dujeychugh jagh nIv yItuHQo'." translation = "There is nothing shameful in falling before a superior enemy." />
          <FactionThreat severity="normal" value="bogh tlhInganpu', SuvwI'pu' moj, Hegh." translation = "Klingons are born, live as warriors, then die." />
          <FactionThreat severity="normal" value="SuvmeH 'ej charghmeH bogh tlhInganpu'." translation = "Klingons are born to fight and conquer." />
          <FactionThreat severity="normal" value="lumbe' tlhInganpu'." translation = "Klingons do not procrastinate." />
          <FactionThreat severity="normal" value="Hem tlhIngan Segh 'ej maHemtaH 'e' wIHech." translation = "Klingons are a proud race, and we intend to go on being proud." />
